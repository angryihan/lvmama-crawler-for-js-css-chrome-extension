{
    "manifest_version": 2,
    "name": "lvmama-crawler-for-js-css-chrome-extension",
    "description": "驴妈妈缓存爬虫-chrome插件版",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Click here!"
    },
    "background": {
        "page":"background.html"
    },
    "permissions": ["http://*.lvmama.com/*","http://*/*","tabs","file:///*"]
}
